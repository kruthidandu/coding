#include <iostream>
//
SinglyLinkedListNode*deleteNode(SinglyLinkedListNode*llist,int position){
    SinglyLinkedListNode*curr=llist;
    
    if(position==0){
        return llist->next;
    }
    for(int i=1;i<position;i++){
        llist=llist->next;
    }
    llist->next=llist->next->next;
    return curr;
}
